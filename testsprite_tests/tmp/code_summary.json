  "features": [
    {
      "name": "AI Job Recommendations",
      "description": "AI-powered job matching system using Kimi K2 that analyzes user profiles and provides personalized job recommendations with match scores and reasoning",
      "files": [
        "frontend/src/lib/ai-service.ts",
        "frontend/src/app/api/ai/recommendations/route.ts",
        "frontend/src/components/ai/JobRecommendations.tsx",
        "frontend/src/app/jobs/page.tsx"
      ],
      "api_doc": {
        "openapi": "3.0.0",
        "info": {
          "title": "AI Job Recommendations API",
          "version": "1.0.0",
          "description": "AI-powered job recommendation system"
        },
        "paths": {
          "/api/ai/recommendations": {
            "post": {
              "summary": "Get AI job recommendations",
              "requestBody": {
                "content": {
                  "application/json": {
                    "schema": {
                      "type": "object",
                      "properties": {
                        "userProfile": {
                          "type": "object",
                          "properties": {
                            "id": {"type": "string"},
                            "skills": {"type": "array", "items": {"type": "string"}},
                            "experience": {"type": "string"},
                            "location": {"type": "string"}
                          }
                        },
                        "jobs": {
                          "type": "array",
                          "items": {
                            "type": "object",
                            "properties": {
                              "id": {"type": "string"},
                              "title": {"type": "string"},
                              "company": {"type": "object"},
                              "location": {"type": "string"}
                            }
                          }
                        }
                      }
                    }
                  }
                }
              },
              "responses": {
                "200": {
                  "description": "Successful response",
                  "content": {
                    "application/json": {
                      "schema": {
                        "type": "object",
                        "properties": {
                          "success": {"type": "boolean"},
                          "recommendations": {
                            "type": "array",
                            "items": {
                              "type": "object",
                              "properties": {
                                "jobId": {"type": "string"},
                                "matchScore": {"type": "number"},
                                "reasoning": {"type": "string"},
                                "keyStrengths": {"type": "array", "items": {"type": "string"}},
                                "potentialConcerns": {"type": "array", "items": {"type": "string"}}
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
